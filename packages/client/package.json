{
  "name": "template-parcel-react",
  "version": "1.0.0",
  "description": "ðŸ“¦ A minimal React Parcel starter template.",
  "type": "module",
  "source": "index.html",
  "scripts": {
    "build": "tsc --noEmit && parcel build",
    "build:report": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "dev": "parcel --port 4900 --cert ./certs/localhost.cert --key ./certs/localhost.key",
    "preview": "http-server dist --port 4901 -S --cert ./certs/localhost.cert --key ./certs/localhost.key",
    "start": "pnpm dev"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.4"
  },
  "devDependencies": {
    "@parcel/config-default": "2.8.0",
    "@parcel/core": "2.8.0",
    "@parcel/reporter-bundle-analyzer": "2.8.0",
    "@parcel/reporter-bundle-buddy": "2.8.0",
    "@parcel/transformer-svg-react": "2.8.0",
    "@parcel/validator-typescript": "2.8.0",
    "@types/parcel-env": "0.0.1",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "http-server": "14.1.1",
    "parcel": "2.8.0",
    "postcss": "8.4.19",
    "postcss-custom-properties": "13.0.0",
    "postcss-import": "15.0.1",
    "postcss-url": "10.1.3",
    "sort-package-json": "2.1.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3"
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true,
      "customMedia": true
    },
    "pseudoClasses": {
      "focusVisible": "focus-visible"
    }
  },
  "alias": {
    "@/*": "./src/$1"
  }
}
